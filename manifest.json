{
  "manifest_version": 3,
  "name": "ChillTool's",
  "description": "Extension for uhmegle.com that grabs your partner's IP address and other cool features!",
  "version": "3.6",
  "icons": {
    "16": "logo/chilltools_16.png",
    "48": "logo/chilltools_48.png",
    "128": "logo/chilltools_128.png"
  },
  "permissions": [
    "activeTab"
  ],
  "host_permissions": [
    "https://uhmegle.com/*"
  ],
  "content_scripts": [
    {
      "matches": [
        "https://uhmegle.com/*"
      ],
      "js": ["inject.js"]
    }
  ],
  "web_accessible_resources": [
    {
      "resources": [
        "scripts/uhmegle.js",
        "scripts/buttons.js",
        "scripts/antip.js"
      ],
      "matches": ["https://uhmegle.com/*"]
    }
  ]
}
